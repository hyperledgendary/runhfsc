{
  "name": "@hyperledgendary/runhfsc",
  "version": "0.0.1",
  "description": "Simple CLI for testing transactions",
  "bin": {
    "runhfsc": "lib/main.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "npm run format && npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledgendary/runhfsc"
  },
  "keywords": [
    "fabric-ledger",
    "Hyperledger Fabric",
    "Fabric Ledger"
  ],
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/node": "^14.0.11",
    "@types/yargs": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.5",
    "chalk": "^4.1.0",
    "cliffy": "^2.3.0",
    "fabric-network": "^2.1.2",
    "js-yaml": "^3.14.0",
    "standard-version": "^8.0.1",
    "yargs": "^15.1.0"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
