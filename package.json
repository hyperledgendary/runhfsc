{
  "name": "@hyperledgendary/runhfsc",
  "version": "0.0.2",
  "description": "Simple CLI for testing transactions",
  "bin": {
    "runhfsc": "lib/main.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "build": "npm run format && npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty && npm shrinkwrap",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperledgendary/runhfsc"
  },
  "keywords": [
    "fabric-ledger",
    "Hyperledger Fabric",
    "Fabric Ledger"
  ],
  "author": "",
  "license": "Apache 2.0",
  "devDependencies": {
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.10.2",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/source-map-support": "^0.5.3",
    "chalk": "^4.1.2",
    "cliffy": "^2.4.3",
    "env-var": "^7.0.1",
    "fabric-network": "^2.2.9",
    "js-yaml": "^4.1.0",
    "source-map-support": "^0.5.19",
    "standard-version": "^9.3.1",
    "yargs": "^17.2.1"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
